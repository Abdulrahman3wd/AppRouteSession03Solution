@model ResetPasswordViewModel
@{
    ViewData["Title"] = "ResetPassword";
    Layout = "~/Views/Shared/_AuthLayout.cshtml";
}

<h1>ResetPassword</h1>

<div class="main-agileinfo">
	<div class="agileits-top alert-info">
		<div asp-validation-summary="ModelOnly"></div>
		<form asp-action="ResetPassword">
			
			<input class="text" asp-for="NewPassword" placeholder="New Password" required="">
			<input class="text w3lpass" asp-for="ConfirmPassword" placeholder="Confirm Password" required="">
			
			<input type="submit" value="Submit">
		</form>
	</div>
</div>
@section ValidationScriptsSection {
	<partial name="_ValidationScriptsPartial" />
}
